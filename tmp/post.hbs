{{!< default}}
{{#post}}
<article class="{{^is "home"}}tag-hash-full-image{{/is}}">
	{{>posts}}
	<div class="section-post wrap">
		<div class="post-wrap {{^if feature_image}}no-image{{/if}}{{^if access}} no-access{{/if}}">
			{{#if access}}
			{{content}}
			{{else}}
			{{#if html}}
			<div class="members-teaser">
				{{{html}}}
			</div>
			{{/if}}
			{{>members/cta}}
			{{/if}}
		</div>
		{{#if access}}
		{{>post_authors}}
		{{/if}}
		<div class="post-meta">
			{{>post_share}}
		</div>
	</div>
</article>
{{>post_navigation}}
<div class="section-disqus">
	{{#if access}}
	{{>services/disqus}}
	{{/if}}
</div>
{{#has tag="#subscribe-form"}}
{{>members/subscribe_form}}
{{/has}}
{{/post}}