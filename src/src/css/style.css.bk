/* RETE FLOW I VANTAGGI */
.vantaggi-title h2{
    margin:0;
}
/* Vantaggi GRID */
.vantaggi-grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    grid-template-rows: repeat(5, 1fr);
    grid-column-gap: 0px;
    grid-row-gap: 0px;
    
    max-width: 680px;
    margin: auto;
    }

    .vantaggi-1 { grid-area: 1 / 2 / 2 / 3; }
    .vantaggi-2 { grid-area: 2 / 1 / 3 / 2; }
    .vantaggi-3 { grid-area: 3 / 2 / 4 / 3; }
    .vantaggi-4 { grid-area: 4 / 1 / 5 / 2; }
    .vantaggi-cta { grid-area: 5 / 1 / 6 / 3; } 
    .vantaggi-bg { grid-area: 1 / 1 / 6 / 3; z-index: -1; } 

    /* Vantaggi CONTENT */
    .vantaggi-grid > div:not(.vantaggi-bg){
        display: flex;
        margin-top:auto; margin-bottom: auto;
        max-width: 450px;
    }
    .vantaggi-grid .box{
        padding-top: 150px;
        position: relative;
        line-height: 1.55;
    }
    .vantaggi-grid h3{
        margin-bottom: 20px;
    }
    .vantaggi-grid p{
        margin-bottom: 0px;
        font-size: 90%;
        line-height: 1.4;
    }

        /* Vantaggi icon */
        .vantaggi-grid .box::after{
            content:'';
            width: 140px; height: 100%;
            background-size: contain; background-repeat: no-repeat;
            transition: 300ms all
        }
        .odd .box::after{
            position: absolute; right: -170px; top:90px;
        }
        .even .box::after{
            position: absolute; left: -190px; top:90px;
        }
        .even .box{
            margin-left: -20px;
        }
        .vantaggi-grid > div:hover .box::after{
            transform: scale(1.1);
        }
        /* Formazione */
        .vantaggi-1 .box::after{
            background-image: url('../assets/img/rete/rete-formazione.png');
        }
        /* Scambio */
        .vantaggi-2 .box::after{
            background-image: url('../assets/img/rete/rete-scambio.png');
        }
        /* Lavoro di squadra */
        .vantaggi-3 .box::after{
            background-image: url('../assets/img/rete/rete-squadra.png');
            background-size: 110%; /* fix png size */
        }
        /* Futuro */
        .vantaggi-4 .box::after{
            background-image: url('../assets/img/rete/rete-futuro.png');
        }
        
        /* Vantaggi CTA */
        .vantaggi-cta{
            display: flex;
            margin: auto;
            color: white;
        }
            .vantaggi-cta span{
                margin-top:220px;
            }
        /* Vantaggi Background */
        .vantaggi-bg img{
            width: 100%;
        }

        .vantaggi-cta video{
            border-radius: 200px !important;
        }
        .vantaggi-cta h1{
            font-size: 1.72rem;
            margin: 0; padding: 0;
        }
        .vantaggi-cta .round{
            width: unset; height: unset;
        }


        @media (max-width: 575.98px) {
            .vantaggi-title h2{
                font-size: 7vw;
            }
            .vantaggi-grid{
                display: flex;
                flex-wrap: wrap;
                padding-top: 50px;
            }
            .vantaggi-grid > div:not(.vantaggi-bg){
                max-width: 100%;
            }
            .vantaggi-bg{
                display: none;
            }
            .vantaggi-grid .box{
                padding-top: 20px;
                padding-left: 100px;
                margin-bottom: 40px;
            }
            .even .box {
                margin-left: 0px;
            }
            .vantaggi-grid .box::after {
                width: 80px;
                height: 100%;
            }
            .odd .box::after {
                position: absolute;
                left: 0px;
                top: 0px;
            }
            .even .box::after {
                position: absolute;
                left: 0px;
                top: 0px;
            }
            .vantaggi-grid h3 {
                margin-bottom: 10px;
            }
            .vantaggi-cta{
                padding:3rem;
                margin-bottom: 3rem !important;
            }
            .vantaggi-cta h1{
                font-size: 1.5rem;
            }
        }

