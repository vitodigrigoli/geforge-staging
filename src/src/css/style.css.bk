:root{
    /* MAIN COLORS */
    --arancio: #fe8005;
    --arancio-chiaro: #FCBA04;
    --blu-scuro: #131C25;
    --blu-chiaro: #27436a;
    --viola-scuro: #862889;
    --viola-chiaro: #AF40B5;
    
    --rosso-scuro: #9A031E;
    --peach: #F07167;
    --azzurro-chiaro: #E7EEF4;
    --grigio-scuro: #f5eee7;
    --grigio-panna: #fff8f2;


    /* VERSIONE LIGHT */
    --light-text-color: #3C4856;
    --light-bg-color: white;
    /* VERSIONE DARK */
    --dark-text-color: white;
    --dark-bg-color: black;

    /* SET LIGHT VERSION */
    --text-color: var(--light-text-color);
    --bg-color: #fff;
  }

/* reset -- valutare reboot.css */
html, body{
    margin:0px;
    padding:0px;
}
blockquote :last-child, li :last-child, p :last-child {
    margin-bottom: 0;
}
*, ::after, ::before {
    box-sizing: border-box;
}



/**********/

html{
    font-size:20px;
}
body{
    font-size:0.9rem;
    background-color: var(--bg-color);
    color: var(--text-color);
    transition: all 300ms;
    font-family: 'Source Sans Pro', sans-serif;
}
main{
    margin:auto;
}

/* Extra small devices (phones, 600px and down) */
@media only screen and (max-width: 600px) {
    html{
        font-size: 15px;
    }
}

/* Small devices (portrait tablets and large phones, 600px and up) */
@media only screen and (min-width: 600px) {
    html{
        font-size: 16px;
    }
}

/* Medium devices (landscape tablets, 768px and up) */
@media only screen and (min-width: 768px) {
    html{
        font-size: 17px;
    }
}

/* Large devices (laptops/desktops, 992px and up) */
@media only screen and (min-width: 992px) {
    html{
        font-size: 18px;
    }
}

/* Extra large devices (large laptops and desktops, 1200px and up) */
@media only screen and (min-width: 1200px) {
    html{
        font-size: 20px;
    }
}

@media screen and (min-width: 1400px) {
    html {
      /*font-size: 24px;*/
      font-size: clamp(1rem, -0.875rem + 10.333333vw, 1.5rem);
    }
  }
  @media screen and (min-width: 1600px) {
    html {
        font-size: clamp(1rem, -0.875rem + 10.333333vw, 1.5rem);
    }
  }
  @media screen and (min-width: 1900px) {
    html {
        font-size: clamp(1.52rem, -0.875rem + 10.333333vw, 1.7rem);
    }
  }


