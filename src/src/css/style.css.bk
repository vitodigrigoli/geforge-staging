/* main elements */
/* NAVBAR */
.main-header {
    position: absolute;
    top: 0;
    z-index: 10000009;
    width: 100%;
    /*background-color: white;*/
    opacity: 0.95;
    /*box-shadow: 0 0 0.25rem rgba(0,0,0,.08),0 0.5rem 0.75rem rgba(0,0,0,0.08);*/
    transition: 1300ms all;
}
.top-bar {
    display: grid;
    grid-template-columns: 1fr auto;
    grid-template-areas: "logo nav";
    /*padding: 0 4vw;*/
    align-items: center;
    margin-left: auto;
    margin-right: auto;
    height: 100%; max-width: 1420px;
    padding: 20px;
}
.logo{
    padding-left: 25px;
}
.logo a{
    display: flex;
}

.logo img{
    transition: 300ms all;
  }
  .logo:hover img{
    transform: scale(1.3);
    perspective-origin: center;
    
  }
.logo-img{
    max-width: 3.8rem;
    transition: 300ms visibility;
}
.nav-main{
    transition: all 0.5s ease;
    display: block;
    grid-area: nav; /* vedi top-bar grid-template-areas */
    width: 100%;
    opacity: 0;
    transition: 1300ms all;
}
.main-menu {
    display: flex;;
    justify-content: center;
    list-style: none;
    margin: 0;
}
.main-menu > li {
    list-style: none;
    font-size: 0.8rem; text-transform: uppercase; font-weight: 600;
    padding: 0 0.3875rem;
    margin:0;

}
.menu-item {
    -webkit-text-decoration: none;
    text-decoration: none;
    border-top: 2px solid transparent;
    color: #100818;
    display: flex;
    align-items: center;
    padding: 1.5rem 1.5rem;
    color: var(--blu-scuro);
    transition: 300ms all;
}
.menu-item:hover{
    color: var(--arancio);
}

.top-bar .cta{
    text-align: right;
}

@media (max-width: 575.98px) {
    .nav-main{
        display: none;
    }
    .logo a{
        padding:1rem 0;
    }
}


