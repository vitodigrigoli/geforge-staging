    .image{
        grid-row: 1/span 5;
        grid-column: 2/span 4;
        transform-origin: center;
    }
    .text{
        grid-row: 9;
        grid-column: 1/span 6;
    }

    .smartphone,
    .image,
    .image video{
        border-radius: 18px;
        background: black;
    }
    .storyThumb{
        align-items: start;
    }
    /* 1 */
    .storyThumb:nth-of-type(2){
        grid-row: 1/span 2;
        grid-column: 1/span 2;
        margin-top: -40px;
        margin-left: 40px;
        z-index: -1;
    }
    /* 2 */
    .storyThumb:nth-of-type(3){
        grid-row: 2/span 2;
        grid-column: 1/span 2;
        margin-top: 55px;
        padding: 20px;
        margin-left: -16px
    }
    /* 3 */
    .storyThumb:nth-of-type(4){
        grid-row: 6/span 2;
        grid-column: 1/span 2;
        margin-top: 15px;
        z-index: -1;
        margin-left: 20px;
    }
    /* 4 */
    .storyThumb:nth-of-type(5){
        grid-row: 4/span 2;
        grid-column: 4/span 3;
        margin-top: -30px;
        padding: 20px;
        margin-left: 20px;
        z-index: -1;
    }
    /* 5 */
    .storyThumb:nth-of-type(6){
        grid-row: 6/span 2;
        grid-column: 1/span 2;
        margin-top: 15px;
        z-index: -1;
        margin-left: 20px;
        display: none;
    }
    
    /* 6 */
    .storyThumb:nth-of-type(7){
        grid-row: 4/span 2;
        grid-column: 4/span 3;
        margin-top: -30px;
        padding: 20px;
        margin-left: 20px;
        z-index: -1;
        display: none;
    }
    
    /* 7 */
    .storyThumb:nth-of-type(8){
        grid-row: 1;
        grid-column: 3/span 2;
        margin-top: 10px;
        align-content: flex-end;
        padding: 20px;
        display: none;
    }

    /* .storyThumb:nth-of-type(2),
    .storyThumb:nth-of-type(3){
        grid-row: 1;
        grid-column: 1/span 2;
