/* SECTIONS */


.section-intro{
    display: grid;
    grid-template-columns: repeat(12, 1fr); /* 12 => 1 */
    grid-template-rows: 1fr;
    grid-column-gap: 50px;
    grid-row-gap: 0px; 
    margin-bottom:100px;
    }
    
    .section-subtitle { grid-area: 1 / 1 / 2 / 7; }
    .section-img { grid-area: 1 / 8 / 2 / 13; } 
    .section-text { grid-area: 1 / 7 / 2 / 13; } 

    .section-content {
        display: flex;
        flex-wrap: wrap;
        justify-content: flex-start;
        align-items: stretch;
        align-content: stretch;
        gap: 50px;
        row-gap: 180px;
        margin-bottom:200px;
        }

    .section-title{
        color: var(--arancio-chiaro);
        font-size: 16px;
        text-transform: uppercase;
        text-align: center;
        margin:auto;
        width: 100%;
        margin-bottom: 40px;
    }
    .section-subtitle{
        position: relative;
        
    }
    section .content-title{
        margin-bottom: 100px;
    }
        ._section-subtitle::after{
            display: block;
            content:url('../assets/title-bg.svg');
            width:100px; height: auto;
            position: absolute;
            z-index: 1;
            top: -25px; left: -45px;
        }
        .section-subtitle h2{
            z-index: 10;
            position: relative;
            font-size: 3rem;
        }
        .section-subtitle p{
            font-size: 1rem;
        }


        @media (max-width: 575.98px) {
            .section-subtitle h2{
                font-size: 8vw;
            }
            .section-intro{
                margin-bottom: 60px;
            }
        }


